import java.util.*;
import java.io.*;
class HelloWorld {
    public static void main(String[] args) 
    {
        Scanner s=new Scanner(System.in);
String str="  java  is  ";
System.out.println(str.length());
String str1=str.trim();
System.out.println(str1.length());
System.out.println(str1);
    }
}
