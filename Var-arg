import java .util.*;
class Test
{
    void show(int... a)
    {
System.out.println("Var-arg method"+Arrays.toString(a));
    }
    public static void main(String[]args)
    {
        Test t1=new Test();
        t1.show(11,22,33);
        t1.show(55,45,66,99);
        t1.show(34,78);
        t1.show(11,22,33,44,55);
        t1.show();
    }
}
output:
Var-arg method[11, 22, 33]
Var-arg method[55, 45, 66, 99]
Var-arg method[34, 78]
Var-arg method[11, 22, 33, 44, 55]
Var-arg method[]
